set show-all-if-ambiguous on
set bell-style none
set editing-mode vi
set keymap vi
$if mode=vi
    # command mode
    set keymap vi-command
    "gg": beginning-of-history
    "G": end-of-history

    # insert mode 
    set keymap vi-insert  
    "jk": vi-movement-mode     
    "\C-l": clear-screen
$endif

